{
	"compilerOptions": {
		"module": "commonjs",
		"esModuleInterop": true,
		"noEmitOnError": true,
		"allowSyntheticDefaultImports": false,
		"jsx": "react",
		"target": "es6",
		"noImplicitAny": false,
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "dist",
		"baseUrl": ".",
		"paths": {
			"*": ["node_modules/*", "src/types/*"]
		}
	},
	"include": ["src/**/*"]
}
