{
	"printWidth": 80,
	"parser": "typescript",
	"tabWidth": 2,
	"useTabs": true,
	"singleQuote": true,
	"bracketSpacing": true,
	"jsxBracketSameLine": true,
	"arrowParens": "always",
	"overrides": [
		{
			"files": "*.json",
			"options": {
				"parser": "json",
				"tabWidth": 2
			}
		},
		{
			"files": ".prettierrc",
			"options": {
				"parser": "json",
				"tabWidth": 2
			}
		}
	]
}
